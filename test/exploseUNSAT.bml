;------------------------------------------------------------------
; Il s'git d'un "exemple" de formule qui semble ne pas terminer.
;           -> <> <> p			branche 1
;  -> []  & + 
;  |        -> <> <> q2			branche 2
; &+
;  |     -> [] <> ~ p			branche 3
;  -> or +
;        |          ->  ~p2		\
;        -> <> [] & +			 branche 4
;					-> p2		/
; Par contre elle termine dans la logique S5
;------------------------------------------------------------------



-M -4 %
	([]	((<> (<> (p)))
		& 
		(<> (<> (q2))))) 
& 
	(([] (<> ([] (~ (p))))) 
	| 
	(<> ([] ((~ (p2)) 
			& 
			(p2))))) 
